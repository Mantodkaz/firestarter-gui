{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "core:path:default",
    "dialog:default",
    {
      "identifier": "http:default",
      "scope": [
        "https://us-west-00-firestarter.pipenetwork.com/*",
        "https://us-west-00-firestarter.pipenetwork.com/api/*"
      ]
    }
  ]
}
